<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.url.shorturl.url.mapper.ShortUrlMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.url.shorturl.url.entity.ShortUrl">
        <id column="id" property="id" />
        <result column="url_key" property="urlKey" />
        <result column="url_value" property="urlValue" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url_key, url_value, create_time
    </sql>

</mapper>
